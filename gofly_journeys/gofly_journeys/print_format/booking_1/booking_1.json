 "html": "<!DOCTYPE html>\n<html>\n \n<head>\n<meta charset=\"UTF-8\">\n<style>\n        @import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;600;700&display=swap');\n \n        body {\n            font-family: 'Montserrat', Arial, sans-serif;\n            color: #333;\n            margin: 20px;\n            line-height: 1.6;\n            background-color: #fff;\n            position: relative;\n        }\n \n        /* === Watermark === */\n        body::before {\n            content: \"\";\n            background: url('/private/files/Gofly Journey.png') no-repeat center center;\n            background-size: 400px;\n            opacity: 0.08;\n            position: fixed;\n            top: -80px;\n            left: 0;\n            width: 100%;\n            height: 100%;\n            z-index: 0;\n            pointer-events: none;\n        }\n \n        /* Header */\n        .header {\n            display: flex;\n            align-items: center;\n            justify-content: space-between;\n            border-bottom: 3px solid #ff6600;\n            padding-bottom: 10px;\n            position: relative;\n            z-index: 1;\n        }\n \n        .header img {\n            height: 70px;\n        }\n \n        h2 {\n            color: #ff6600;\n            text-transform: uppercase;\n            font-weight: 700;\n            margin-top: 10px;\n        }\n \n        /* Info Section */\n        .info-section {\n            margin: 20px 0;\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n            gap: 12px;\n            position: relative;\n            z-index: 1;\n        }\n \n        .info-box {\n            background: #f9f9f9;\n            padding: 14px 16px;\n            border-radius: 12px;\n            box-shadow: 0 2px 5px rgba(0, 0, 0, 0.08);\n            transition: all 0.3s ease;\n        }\n \n        .info-box:hover {\n            transform: translateY(-3px);\n            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.15);\n        }\n \n        .info-box strong {\n            display: block;\n            color: #ff6600;\n            font-size: 14px;\n            font-weight: 700;\n            margin-bottom: 5px;\n        }\n \n        .info-box span {\n            font-size: 15px;\n            font-weight: 600;\n            color: #333;\n        }\n \n        /* Table */\n        table {\n            width: 100%;\n            border-collapse: collapse;\n            margin: 25px 0;\n            border-radius: 10px;\n            overflow: hidden;\n            position: relative;\n            z-index: 1;\n        }\n \n        th,\n        td {\n            border: 1px solid #eee;\n            padding: 10px 12px;\n            text-align: left;\n        }\n \n        th {\n            background-color: #ff6600;\n            color: white;\n            font-weight: 600;\n        }\n \n        tr:nth-child(even) {\n            background-color: #fafafa;\n        }\n \n        h3 {\n            color: #ff6600;\n            margin-top: 25px;\n            font-weight: 700;\n            position: relative;\n            z-index: 1;\n        }\n \n        /* Section Cards */\n        .section-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(260px, 1fr));\n            gap: 15px;\n            margin-top: 25px;\n            position: relative;\n            z-index: 1;\n        }\n \n        .section-card {\n            background: linear-gradient(135deg, #fff8f2, #ffe4d1);\n            border: 1px solid #ffd2a1;\n            border-left: 6px solid #ff6600;\n            border-radius: 14px;\n            padding: 18px 20px;\n            box-shadow: 0 2px 6px rgba(0, 0, 0, 0.1);\n            transition: transform 0.3s ease, box-shadow 0.3s ease;\n        }\n \n        .section-card:hover {\n            transform: translateY(-4px);\n            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.15);\n        }\n \n        .section-card h4 {\n            color: #ff6600;\n            font-weight: 700;\n            font-size: 15px;\n            margin-bottom: 6px;\n            text-transform: uppercase;\n        }\n \n        .section-card p {\n            font-size: 16px;\n            font-weight: 600;\n            color: #333;\n            margin: 0;\n        }\n \n        /* Terms Box */\n        .terms-box {\n            background: #f8faff;\n            border: 1px solid #dce9ff;\n            border-radius: 10px;\n            padding: 15px 20px;\n            margin-top: 25px;\n        }\n \n        .terms-box ul {\n            margin-top: 10px;\n            padding-left: 20px;\n        }\n \n        .terms-box li {\n            margin-bottom: 6px;\n        }\n \n        @media print {\n            body::before {\n                opacity: 0.1;\n                top: -60px;\n            }\n \n            .section-card:hover {\n                box-shadow: none;\n                transform: none;\n            }\n        }\n</style>\n</head>\n \n<body> <!-- Header -->\n<div class=\"header\"> <img src=\"/private/files/Gofly Journey.png\" alt=\"GoFly Journey Logo\">\n<h2>Booking Summary</h2>\n</div> <!-- Package Info -->\n<div class=\"info-section\">\n<div class=\"info-box\"> <strong>Tour Package:</strong> <span>{{ doc.tour_package }}</span> </div>\n<div class=\"info-box\"> <strong>Start Date:</strong> <span>{{ doc.start_date }}</span> </div>\n<div class=\"info-box\"> <strong>End Date:</strong> <span>{{ doc.end_date }}</span> </div>\n<div class=\"info-box\"> <strong>Package Status:</strong> <span>{{ doc.package_status }}</span> </div>\n<div class=\"info-box\"> <strong>Amount:</strong> <span>\u20b9 {{ \"{:,.2f}\".format(doc.amount) }}</span> </div>\n</div> <!-- Booking Info -->\n<h3>Booking Details</h3>\n<div class=\"section-grid\">\n<div class=\"section-card\">\n<h4>Customer</h4>\n<p>{{ doc.customer }}</p>\n</div>\n<div class=\"section-card\">\n<h4>Booking Date</h4>\n<p>{{ doc.booking_date }}</p>\n</div>\n<div class=\"section-card\">\n<h4>Return Date</h4>\n<p>{{ doc.return_date }}</p>\n</div>\n<div class=\"section-card\">\n<h4>Country</h4>\n<p>{{ doc.package_country }}</p>\n</div>\n<div class=\"section-card\">\n<h4>State</h4>\n<p>{{ doc.package_state }}</p>\n</div>\n<div class=\"section-card\">\n<h4>Booking Status</h4>\n<p>{{ doc.booking_status }}</p>\n</div>\n<div class=\"section-card\">\n<h4>Sender Email</h4>\n<p>{{ doc.sender_email }}</p>\n</div>\n</div> <!-- Visa Members -->\n<h3>Visa Details</h3>\n<table>\n<thead>\n<tr>\n<th>No.</th>\n<th>Full Name</th>\n<th>Age</th>\n<th>Gender</th>\n<th>Nationality</th>\n<th>Passport Number</th>\n<th>Visa Required</th>\n</tr>\n</thead>\n<tbody> {% for row in doc.booking_members %} <tr>\n<td>{{ loop.index }}</td>\n<td>{{ row.full_name }}</td>\n<td>{{ row.age }}</td>\n<td>{{ row.gender }}</td>\n<td>{{ row.nationality }}</td>\n<td>{{ row.passport_number }}</td>\n<td>{{ row.visa_required }}</td>\n</tr> {% endfor %} </tbody>\n</table>\n<div style=\"text-align: right; font-weight: 600;\">Total Visa Amount: \u20b9 {{ \"{:,.2f}\".format(doc.total_visa_fee or 0)\n        }}</div> <!-- Footer -->\n<div class=\"footer\"> {{ frappe.db.get_value(\"Letter Head\", doc.letter_head, \"footer\") or \"\" }} </div>\n</body>\n \n</html>",
